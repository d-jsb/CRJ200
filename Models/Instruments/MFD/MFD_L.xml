<?xml version="1.0"?>

<PropertyList>

	<path>mfd_l.ac</path>
	
	
    <animation>
        <type>pick</type>
        <object-name>ND.screen</object-name>
        <action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>nasal</command>
				<script>canvas_mfd.showMfd();</script>
			</binding>
        </action>
    </animation>

	<animation>
		<type>material</type>
		<object-name>ND.screen</object-name>
		<emission>
			<red>1</red>
			<green>1</green>
			<blue>1</blue>
			<factor-prop>controls/lighting/DU/du2</factor-prop>
		</emission>
	</animation>
   
</PropertyList>
