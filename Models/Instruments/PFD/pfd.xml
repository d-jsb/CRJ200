<?xml version="1.0" encoding="UTF-8" ?>

<!-- Bombardier CRJ700 series -->
<!-- Primary flight display -->

<PropertyList>

	<path>pfd.ac</path>

	<!-- Lightmaps -->
	<effect>
		<inherits-from>Aircraft/CRJ700-family/Models/Effects/CRJ700lightmap</inherits-from>
		<parameters>
			<lightmap-factor-prop>sim/model/lights/panel-lightmap</lightmap-factor-prop>
			<lightmap-factor n="0">
				<use alias="../../lightmap-factor-prop" />
			</lightmap-factor>
			<lightmap-factor n="1">
				<use alias="../../lightmap-factor-prop" />
			</lightmap-factor>
			<lightmap-factor n="2">
				<use alias="../../lightmap-factor-prop" />
			</lightmap-factor>
			<texture n="3">
				<image alias="../../../../params/lightmap-texture-path" />
			</texture>
		</parameters>
		<object-name>Border</object-name>
	</effect>

</PropertyList>
